   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "0.029531204310524296\n"
     ]
    }
   ],
   "source": [
    "import math \n",
    "x=5 \n",
    "n=50 \n",
    "y=(1/x)**x* ((x-1)/x)**(n-x) * (math.factorial(n)/(math.factorial(x)*math.factorial(n-x))) \n",
    "print(y)"
   ]
  }
